{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "plugin-block/plugin-editable",
	"version": "1.0",
	"title": "Block Editable",
	"icon": "edit",
	"category": "text",
	"keywords": [ "Editable", "Gutenberg" ],
	"description": "Description...",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"source": "text",
			"selector": ".title"
		},
		"content": {
			"type": "string",
			"source": "text",
			"selector": ".text"
		},
		"image": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		}
	},
	"textdomain": "plugin-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
